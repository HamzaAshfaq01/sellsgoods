{
  "name": "sellsgoods",
  "version": "2.0.0",
  "type": "module",
  "description": "Application built with the MERN stack",
  "main": "server.js",
  "scripts": {
    "start": "node api/index.js",
    "server": "nodemon api/index.js",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node api/seeder",
    "data:destroy": "node api/seeder -d",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend",
    "generate-toc": "markdown-toc -i readme.md"
  },
  "dependencies": {
    "antd": "^5.24.5",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "lucide-react": "^0.483.0",
    "mongoose": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "concurrently": "^7.6.0",
    "markdown-toc": "^1.2.0",
    "nodemon": "^2.0.21"
  }
}
